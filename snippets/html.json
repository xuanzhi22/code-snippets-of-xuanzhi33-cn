    "Cookie操作函数": {
        "prefix": "cookie",
        "scope": "javascript",
        "description": "",
        "body": [
            "        function getCookie(cname) {\r",
            "            var name = cname + \"=\";\r",
            "            var ca = document.cookie.split(';');\r",
            "            for (var i = 0; i < ca.length; i++) {\r",
            "                var c = ca[i].trim();\r",
            "                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);\r",
            "            }\r",
            "            return \"\";\r",
            "        }\r",
            "        function delCookie(name) {\r",
            "            document.cookie = name + \"=; expires=Thu, 01 Jan 1970 00:00:00 GMT\";\r",
            "        }"
        ]
    },
    "获取URL查询参数": {
        "prefix": "getparam",
        "scope": "javascript",
        "description": "",
        "body": [
            "    function GetUrlByParamName(name) {",
            "      var reg = new RegExp(\"(^|&)\" + name + \"=([^&]*)(&|$)\");",
            "      var URL = decodeURI(window.location.search);",
            "      var r = URL.substr(1).match(reg);",
            "      if (r != null) {",
            "        //decodeURI() 函数可对 encodeURI() 函数编码过的 URI 进行解码  ",
            "        return decodeURI(r[2]);",
            "      };",
            "      return null;",
            "    }"
        ]
    },
